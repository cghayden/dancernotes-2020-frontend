    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "apollo-upload-client": "^14.1.3",
    "babel-plugin-styled-components": "^1.10.6",
    "cookie": "^0.3.1",
    "downshift": "^6.1.0",
    "framer-motion": "^2.9.4",
    "graphql": "^14.5.6",
    "graphql-tag": "^2.10.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
